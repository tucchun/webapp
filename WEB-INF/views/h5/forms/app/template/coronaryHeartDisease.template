            <div class="pull-right">{{= it.coronHeight}}<span class="unit">cm</span></div>
          </div>
          <div class="item clearfix">
            <div class="pull-left">眼底</div>
            <div class="pull-right">{{= it.coronFundusOculi}}</div>
          </div>
          <div class="item clearfix">
            <div class="pull-left">体重</div>
            <div class="pull-right">{{= it.coronWeight}}<span class="unit">kg</span></div>
          </div>
          <div class="item clearfix">
            <div class="pull-left">浮肿</div>
            <div class="pull-right">{{= it.coronEdema}}</div>
          </div>
        </div>
      </div>
      <div class="container flex horizental">
        <div class="flex vertical justify-center align-center border-right bgWhite w60">
          <div>检</div>
          <div>查</div>
        </div>
        <div class="container">
          <div class="item clearfix">
            <div class="pull-left">心电图</div>
            <div class="pull-right">{{= it.coronInspectCardiogram}}</div>
          </div>
          <div class="item clearfix">
            <div class="pull-left">LDL</div>
            <div class="pull-right">{{= it.coronInspectLdl}}<span class="unit">%</span></div>
          </div>
          <div class="item clearfix">
            <div class="pull-left">血糖</div>
            <div class="pull-right">{{= it.coronInspectGlucose}}<span class="unit">mmol/L</span></div>
          </div>
          <div class="item clearfix">
            <div class="pull-left">LDH</div>
            <div class="pull-right">{{= it.coronInspectLdh}}<span class="unit">%</span></div>
          </div>
          <div class="item clearfix">
            <div class="pull-left">TC</div>
            <div class="pull-right">{{= it.coronInspectTc}}<span class="unit">mmol/L</span></div>
          </div>
          <div class="item clearfix">
            <div class="pull-left">CK-MB</div>
            <div class="pull-right">{{= it.coronInspectCkMb}}</div>
          </div>
          <div class="item clearfix">
            <div class="pull-left">TG</div>
            <div class="pull-right">{{= it.coronInspectTg}}<span class="unit">mmol/L</span></div>
          </div>
          <div class="item clearfix">
            <div class="pull-left">HDL</div>
            <div class="pull-right">{{= it.coronInspectHdl}}<span class="unit">mmol/L</span></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="item clearfix">
    <div class="pull-left">并发症</div>
    <div class="pull-right">{{= it.complicationRecordStr}}</div>
  </div>
